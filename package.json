{
  "name": "holo-apollo",
  "version": "1.0.0",
  "description": "Craft Art Marketplace",
  "engines": {
    "node": "10.10.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "test": "env NODE_ENV=test NODE_PATH=./frontend/src mocha --reporter tap --require babel-register --require ./frontend/test/setup.js --require ignore-styles --recursive ./frontend/test",
    "test-coverage": "nyc npm test",
    "start": "webpack --progress -w --config frontend/webpack.config.js",
    "build-dev": "webpack --config frontend/webpack.config.js",
    "build": "webpack --config frontend/webpack.prod.config.js",
    "postinstall": "npm run build-dev && npm run build"
  },
  "keywords": [],
  "author": "Valerii Kovalchuk",
  "repository": {
    "type": "git",
    "url": "https://github.com/pannkotsky/holo-apollo"
  },
  "license": "ISC",
  "devDependencies": {
    "ajv": "^6.5.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.6.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.6.1",
    "file-loader": "^2.0.0",
    "hoek": "^4.2.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.6.2",
    "less": "^3.8.1",
    "less-loader": "^4.0.5",
    "mocha": "^5.0.1",
    "nyc": "^11.4.1",
    "react-test-renderer": "^16.5.1",
    "sinon": "^4.3.0",
    "style-loader": "^0.20.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^0.6.2",
    "url-parse": "^1.4.3",
    "webpack": "^4.19.1",
    "webpack-bundle-tracker": "^0.3.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "classnames": "^2.2.5",
    "material-ui": "^0.20.0",
    "react": "^16.5.1",
    "react-autobind": "^1.0.6",
    "react-dom": "^16.5.1",
    "react-form": "^2.16.2"
  },
  "nyc": {
    "include": [
      "frontend/src/**/*.js"
    ]
  }
}
